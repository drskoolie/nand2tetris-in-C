compile: 
	#!/usr/bin/env bash
	for file in src/*.c;
	do
		gcc -c -Iinclude $file -o obj/$(basename $file .c).o
	done

	gcc obj/*.o -o ./bin/nand

test:
	#!/usr/bin/env bash
	for file in test/*c;
	do
		gcc -c $file -o /obj/$(basename $file .c).o
	done



clean:
	@rm -rf obj/*
	@rm -rf bin/*
	@echo "Cleanup completed"
